

<div class= "page-header">
  <h1>Edit <%= @person.full_name %> a Location</h1>
</div>



<%= form_for [@person, @assignment], html: {class: 'form-horizontal'} do |f| %>

<div class= "form-group">
  <%= f.label :location_id %>
  <%= f.collection_select :location_id, Location.all, :id, :name, {iclude_black: true}, {class: "form-control"} %>
</div>

<div class= "form-group">
  <%= f.label :role %>
  <%= f.text_field :role, class: "form-control" %>
</div>

<div class= "form-group">
  <%= f.submit 'Update', class: 'btn btn-primary' %>
</div>
<% end %>
